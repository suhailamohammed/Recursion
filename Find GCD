#include <iostream>

using namespace std;
int HCF(int,int);
int main()
{
    //variable declaration
    int x,y,h,i;
    
    //input from user
    cout<<"Enter left limit (x): ";
    cin>>x;
    cout<<"Enter right limit (y): ";
    cin>>y;
    
    //output to user
    cout<<"HCF = "<<HCF(x,y);

    return 0;
}
//definning the function
int HCF(int x, int y){
    int i, h, temp;
    
    if( x < y){
        
        return HCF(y,x);
    }
    if(y != 0){
         return HCF(y,x%y);
    }
    else{
        return x;
    }
  
 
    
}
